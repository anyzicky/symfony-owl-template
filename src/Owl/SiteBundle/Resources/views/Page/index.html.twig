        <div class="slider" id="home">

            <div class="container">
                <! SLIDESHOW >
                <div class="flexslider">
                    <ul class="slides">
                        {% for slide in slides %}
                            <li>
                                <div class="sixteen columns slide{{ slide.id }}">
                                    <p class="center"><img class="scale-with-grid" src="images/{{ slide.image }}" height="500px" width="500px" alt="logo big"></p>
                                    <h1 class="white">{{ slide.title }}</h1>
                                    {% if(slide.descr) %}
                                        <h3 class="white"><span>{{ slide.descr }}</span></h3>
                                    {% endif %}
                                </div>
                            </li>
                        {% endfor %}
                    </ul>
                </div>
            </div>
        </div>
    {% endif %}

